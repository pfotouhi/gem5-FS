#!/bin/sh

cd /benchs/parsec
export OMP_NUM_THREADS=64
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/benchs/
./x264 --quiet --qp 20 --partitions b8x8,i4x4 --ref 5 --direct auto --b-pyramid --weightb --mixed-refs --no-fast-pskip --me umh --subme 7 --analyse b8x8,i4x4 --threads 64 -o eledream.264 /benchs/inputs/eledream_640x360_128.y4m
echo "Done running x264 large"
/sbin/m5 exit
/sbin/m5 exit
